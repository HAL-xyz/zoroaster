{
  "TriggerName": "All transactions with a value bigger than X and smaller than Y",
  "TriggerType": "WatchTransactions",
  "Filters": [
    {
      "FilterType": "BasicFilter",
      "ParameterName": "Value",
      "Condition": {
        "Predicate": "BiggerThan",
        "Attribute": "300000000000000000"
      }
    },
    {
      "FilterType": "BasicFilter",
      "ParameterName": "Value",
      "Condition": {
        "Predicate": "SmallerThan",
        "Attribute": "500000000000000000"
      }
    }
  ]
}
