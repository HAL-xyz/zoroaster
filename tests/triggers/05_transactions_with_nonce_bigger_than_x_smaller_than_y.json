{
  "TriggerName": "All transactions within a nonce from X to Y",
  "TriggerType": "WatchTransactions",
  "Filters": [
    {
      "FilterType": "BasicFilter",
      "ParameterName": "Nonce",
      "Condition": {
        "Predicate": "BiggerThan",
        "Attribute": "10"
      }
    },
    {
      "FilterType": "BasicFilter",
      "ParameterName": "Nonce",
      "Condition": {
        "Predicate": "SmallerThan",
        "Attribute": "12"
      }
    }
  ]
}
