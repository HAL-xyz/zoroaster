{
  "TriggerName": "All transactions with Gas between X and Y",
  "TriggerType": "WatchTransactions",
  "Filters": [
    {
      "FilterType": "BasicFilter",
      "ParameterName": "Gas",
      "Condition": {
        "Predicate": "SmallerThan",
        "Attribute": "600000"
      }
    },
    {
      "FilterType": "BasicFilter",
      "ParameterName": "Gas",
      "Condition": {
        "Predicate": "BiggerThan",
        "Attribute": "500000"
      }
    }
  ]
}
