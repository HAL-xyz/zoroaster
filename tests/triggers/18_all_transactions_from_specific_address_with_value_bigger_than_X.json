{
  "TriggerName": "All transactions from a specif address and with a value bigger than X",
  "TriggerType": "WatchTransactions",
  "Filters": [
    {
      "FilterType": "BasicFilter",
      "ParameterName": "From",
      "Condition": {
        "Predicate": "Eq",
        "Attribute": "0x58321230a0A246CA94533D0917505D7877F0D61a"
      }
    },
    {
      "FilterType": "BasicFilter",
      "ParameterName": "Value",
      "Condition": {
        "Predicate": "BiggerThan",
        "Attribute": "100000000000000000"
      }
    }
  ]
}
