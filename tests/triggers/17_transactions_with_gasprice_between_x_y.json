{
  "TriggerName": "All transactions with GasPrice between X and Y",
  "TriggerType": "WatchTransactions",
  "Filters": [
    {
      "FilterType": "BasicFilter",
      "ParameterName": "GasPrice",
      "Condition": {
        "Predicate": "BiggerThan",
        "Attribute": "33100000000"
      }
    },
    {
      "FilterType": "BasicFilter",
      "ParameterName": "GasPrice",
      "Condition": {
        "Predicate": "SmallerThan",
        "Attribute": "50000000000"
      }
    }
  ]
}
